<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #808080;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ucf.assignments.AddTaskController">
    <children>
        <AnchorPane prefHeight="70.0" prefWidth="600.0" style="-fx-background-color: #FFA500;">
            <children>
                <Label layoutX="64.0" layoutY="13.0" text="YOUR TO DO LIST:" textAlignment="CENTER" textFill="#fdfdfd">
                    <font>
                        <Font name="Impact" size="35.0" />
                    </font>
                </Label>
                <ImageView fitHeight="56.0" fitWidth="50.0" layoutX="14.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@baseline_list_alt_white_24dp.png" />
                    </image>
                </ImageView>
            </children>
        </AnchorPane>
        <Label layoutX="14.0" layoutY="82.0" text="No tasks for this list yet." textAlignment="CENTER" textFill="#fdfdfd">
            <font>
                <Font name="Impact" size="18.0" />
            </font>
        </Label>
        <Label layoutX="261.0" layoutY="221.0" text="Add a task." textAlignment="CENTER" textFill="#fdfdfd">
            <font>
                <Font name="Impact" size="18.0" />
            </font>
        </Label>
        <Button layoutX="271.0" layoutY="171.0" mnemonicParsing="false">
            <graphic>
                <ImageView fx:id="addTaskButton" fitHeight="50.0" fitWidth="42.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@baseline_add_circle_white_24dp.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
    </children>
</AnchorPane>